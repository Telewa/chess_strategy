pipeline {
	agent any
	stages {
        stage('Build Backend') {
            steps {
                sh 'docker build -t chesser:1.0.0 backend/chesser'
            }
        }

        stage('Build Frontend') {
            steps {
                sh 'docker build -t chesser-frontend:1.0.0 frontend'
            }
        }

        stage('Build Frontend') {
            steps {
                sh 'docker images | grep chesser'
            }
        }
    }
}